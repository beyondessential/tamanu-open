{
  "extends": ["../../common.tsconfig.json"],
  "include": [
    "src/**/*.ts",
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "strict": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": false,
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "skipLibCheck": true,
    "target": "es2020"
  }
}
