    "reactotron-redux": "^3.1.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.0",
    "styled-system": "^5.1.2",
    "typeorm": "^0.2.31",
    "yup": "^0.28.0"
